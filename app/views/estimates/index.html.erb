<p id="notice"><%= notice %></p>

<h1>Estimates</h1>

<table class="table table-striped table-bordered">
    <thead class="thead-inverse">
        <tr>
            <th>Id</th>
            <th>Student</th>
            <th>Problem</th>
            <th>Session</th>
            <th>Estimated time</th>
            <th>Response timing</th>
        </tr>
    </thead>

    <tbody>
        <% @estimates.each do |estimate| %>
            <tr>
                <td><%= estimate.id %></td>
                <td><%= estimate.student_id %></td>
                <td><%= estimate.problem_id %></td>
                <td><%= estimate.session_id %></td>
                <td><%= estimate.estimated_time %></td>
                <td><%= estimate.response_timing %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Estimate', new_estimate_path %>
