<p id="notice"><%= notice %></p>

<h1>Estimates</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Problem</th>
      <th>Session</th>
      <th>Estimated time</th>
      <th>Response timing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estimates.each do |estimate| %>
      <tr>
        <td><%= estimate.student_id %></td>
        <td><%= estimate.problem_id %></td>
        <td><%= estimate.session_id %></td>
        <td><%= estimate.estimated_time %></td>
        <td><%= estimate.response_timing %></td>
        <td><%= link_to 'Show', estimate %></td>
        <td><%= link_to 'Edit', edit_estimate_path(estimate) %></td>
        <td><%= link_to 'Destroy', estimate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estimate', new_estimate_path %>
