<p id="notice"><%= notice %></p>

<h1>Starts</h1>

<table class="table table-striped table-bordered">
  <thead class="thead-inverse">
    <tr>
        <th>Id</th>
      <th>Student</th>
      <th>Problem</th>
      <th>Session</th>
      <th>Response timing</th>
    </tr>
  </thead>

  <tbody>
    <% @starts.each do |start| %>
      <tr>
          <td><%= start.id %></td>
        <td><%= start.student_id %></td>
        <td><%= start.problem_id %></td>
        <td><%= start.session_id %></td>
        <td><%= start.response_timing %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Start', new_start_path %>
