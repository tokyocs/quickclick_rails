<p id="notice"><%= notice %></p>

<h1>Answers</h1>

<table class="table table-striped table-bordered">
    <thead class="thead-inverse">
        <tr>
            <th>Id</th>
            <th>Student</th>
            <th>Problem</th>
            <th>Session</th>
            <th>Answer</th>
            <th>Response timing</th>
        </tr>
    </thead>

    <tbody>
        <% @answers.each do |answer| %>
            <tr>
                <td><%= answer.id %></td>
                <td><%= answer.student_id %></td>
                <td><%= answer.problem_id %></td>
                <td><%= answer.session_id %></td>
                <td><%= answer.answer %></td>
                <td><%= answer.response_timing %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Answer', new_answer_path %>
