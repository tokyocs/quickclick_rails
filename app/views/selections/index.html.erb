<p id="notice"><%= notice %></p>

<h1>Selections</h1>

<table class="table table-striped table-bordered">
    <thead class="thead-inverse">
        <tr>
            <th>Id</th>
            <th>Student</th>
            <th>Problem</th>
            <th>Session</th>
            <th>Answer</th>
            <th>Response timing</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @selections.each do |selection| %>
            <tr>
                <td><%= selection.id %></td>
                <td><%= selection.student_id %></td>
                <td><%= selection.problem_id %></td>
                <td><%= selection.session_id %></td>
                <td><%= selection.answer %></td>
                <td><%= selection.response_timing %></td>
                <td><%= link_to 'Show', selection %></td>
                <td><%= link_to 'Edit', edit_selection_path(selection) %></td>
                <td><%= link_to 'Destroy', selection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Selection', new_selection_path %>
